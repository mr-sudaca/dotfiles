#THIS MUST BE AT THE END OF THE FILE FOR SDKMAN TO WORK!!!
export SDKMAN_DIR="/Users/jhon/.sdkman"
[[ -s "/Users/jhon/.sdkman/bin/sdkman-init.sh" ]] && source "/Users/jhon/.sdkman/bin/sdkman-init.sh"

